#Trees [zoom>17]{
  marker-file: url("tree10.svg");
  marker-allow-overlap: true;
  marker-ignore-placement: true;
  marker-transform: scale(00.1,00.1);
  marker-width: 200;
  marker-fill-opacity: .2;
  //image-filters: agg-stack-blur(4,4);
}

